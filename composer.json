{
  "name": "iampersistent/webonxy-middleware",
  "type": "library",
  "description": "Webonxy GraqhQL middleware",
  "keywords": [
    "GraphQl",
    "PSR-7",
    "PSR-15",
    "webonxy"
  ],
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1 || ^8.0",
    "psr/container": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^1.0",
    "webonyx/graphql-php": "^14.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0.0",
    "codeception/module-asserts": "dev-master",
    "phpspec/prophecy": "~1.0",
    "laminas/laminas-diactoros": "^2.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "IamPersistent\\GraphQL\\Middleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
